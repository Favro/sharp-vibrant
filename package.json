    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/lodash": "^4.14.194",
    "@types/mocha": "^10.0.1",
    "@types/table": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "chai": "^4.3.7",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
